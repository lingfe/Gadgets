button[type="primary"] {
  color: #fff;
  background-color: #14bac8;
  border-radius: 8px;
}

.button-hover[type="primary"] {
  background-color: #14bac8;
}

button::after {
  content: " ";
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: 1px solid #e8efef;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  border-radius: 10px;
}