<!--miniprogram/pages/index/lswl/lswl_index/hq_list/my_hq_list/check_menu_type/check_menu_type.wxml-->
<view class='page'>

  <!-- 菜单 start -->
  <view class='menu_heng'>
    <navigator url="../../../hq/hq?menu_id={{item._id}}" 
    wx:for="{{type_menu}}"  wx:key="{{item._id}}" class='menu_heng_navigator'>
       <image class='menu_heng_img' src='{{item.menu_img}}'></image>
       <view class='menu_heng_text'>{{item.menu_name}}</view>
       <view class='menu_heng_v'>></view>
    </navigator>
  </view>
  <!-- 菜单 end -->
</view>
